{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte", "@ianvs/prettier-plugin-sort-imports"],
	"importOrder": [
		"<TYPES>",
		"^(svelte|svelte/.*)$",
		"^@sveltejs/(.*)$",
		"^\\$app/(.*)$",
		"^@xyflow/svelte$",
		"^@skeletonlabs/(.*)$",
		"^@tabler/(.*)$",
		"<THIRD_PARTY_MODULES>",
		"",
		"^\\$lib/paraglide/(.*)$",
		"",
		"^\\$lib/classes/(.*)$",
		"^\\$lib/components/(.*)$",
		"^\\$lib/map/(.*)$",
		"^\\$lib/stores/(.*)$",
		"^\\$lib/utils/(.*)$",
		"^\\$lib/(.*)$",
		"",
		"^[./].*\\.svelte$",
		"^[./]",
		"",
		"^.+\\.css$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
